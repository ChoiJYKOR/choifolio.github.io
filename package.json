{
  "name": "juyeong-portfolio",
  "version": "1.0.0",
  "type": "module",
  "description": "최주영의 공장자동화 포트폴리오 웹사이트",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.js",
    "dev:mock": "concurrently \"npm run dev:client\" \"npm run mock:server\"",
    "mock:server": "node server/mock-server.js",
    "build": "vite build && npm run copy:404",
    "copy:404": "cp 404.html dist/404.html 2>/dev/null || copy 404.html dist\\404.html 2>nul || echo '404.html 복사 실패 (수동으로 복사해주세요)'",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "setup-admin": "node server/setup-admin.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@editorjs/code": "^2.9.3",
    "@editorjs/editorjs": "^2.31.0",
    "@editorjs/header": "^2.8.8",
    "@editorjs/list": "^2.0.8",
    "@editorjs/paragraph": "^2.11.7",
    "@lexical/code": "^0.37.0",
    "@lexical/html": "^0.37.0",
    "@lexical/link": "^0.37.0",
    "@lexical/list": "^0.37.0",
    "@lexical/react": "^0.37.0",
    "@lexical/rich-text": "^0.37.0",
    "@lexical/utils": "^0.37.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/three": "^0.180.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "framer-motion": "^10.16.4",
    "gsap": "^3.13.0",
    "helmet": "^8.1.0",
    "i18next": "^25.6.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lexical": "^0.37.0",
    "mongoose": "^8.0.0",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.0.0",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.18.0",
    "react-tooltip": "^5.29.1",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/joi": "^17.2.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
